<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:osu_MusicPlayer"
        xmlns:System="clr-namespace:System;assembly=mscorlib" x:Name="serch" x:Class="osu_MusicPlayer.Window2"
        mc:Ignorable="d"
        Title="Osu! Music Player - 検索" Height="450" Width="597" ResizeMode="CanMinimize">
    <Grid>
        <Button x:Name="Button_serch" Content="検索" HorizontalAlignment="Left" Margin="477,11,0,0" VerticalAlignment="Top" Width="75"/>
        <Label x:Name="Label_Text" Content="タイトル・アーティスト・タグでの検索ができます。" HorizontalAlignment="Left" Margin="10,7,0,0" VerticalAlignment="Top" Width="449" Height="28" Foreground="#FFABADB3" VerticalContentAlignment="Center"/>
        <TextBox x:Name="Text" HorizontalAlignment="Left" Height="23" Margin="10,11,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="449" TextChanged="Text_TextChanged" Foreground="Black" Background="#00000000"/>
        <ListBox x:Name="ListBox_Result" HorizontalAlignment="Left" Height="361" Margin="10,39,0,0" VerticalAlignment="Top" Width="562"/>
    </Grid>
</Window>
